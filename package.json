{
  "name": "@matrunchyk/vue3-tel-input",
  "version": "1.0.0",
  "description": "International Telephone Input with Vue",
  "author": "Serhii Matrunchyk <serhii@matrunchyk.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/matrunchyk/vue3-tel-input.git"
  },

  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/vue3-tel-input.umd.cjs",
  "module": "./dist/vue3-tel-input.js",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/vue3-tel-input.js",
      "require": "./dist/vue3-tel-input.umd.cjs"
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "libphonenumber-js": "^1.10.13"
  },
  "peerDependencies": {
    "vue": "^3.2.39",
    "click-outside-vue3": "^4.0.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^16.11.59",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-typescript": "^11.0.1 ",
    "click-outside-vue3": "^4.0.1",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vite-plugin-static-copy": "^0.9.0",
    "vue-tsc": "^0.40.13"
  },
  "keywords": [
    "vue",
    "telephone",
    "phone",
    "vue-tel-input",
    "phone input",
    "telephone input",
    "input",
    "international phone"
  ]
}
